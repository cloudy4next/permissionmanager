{
    "name": "cloudy4next/permissionmanager",
    "description": "Users and permissions management interface for Laravel 5 using Backpack CRUD.",
    "keywords": [
        "tabacitu",
        "laravel backpack",
        "backpack permission",
        "backpack roles",
        "backpack user management",
        "updivision",
        "backpack",
        "manage permission",
        "manage roles",
        "manage users",
        "users roles admin"
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "spatie/laravel-permission": "^5.0|^4.0|^3.0",
        "cloudy4next/crud": "^2.0.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url":  "git@github.com:cloudy4next/crud.git"
        }
    ],
    "require-dev": {
        "phpunit/phpunit" : "^9.0||^7.0",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Backpack\\PermissionManager\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Backpack\\PermissionManager\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Backpack\\PermissionManager\\PermissionManagerServiceProvider"
            ]
        }
    }
}
